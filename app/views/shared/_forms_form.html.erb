<%= form_for(@form) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, placeholder: "FirstName LastName"%>
  </div>
  <div class="field">
    <%= f.label :dob , "Date of Birth"%><br />
    <%= f.date_select :dob %>
  </div>
  <div class="field">
    <%= f.label :address, placeholder: "Address" %><br />
    <%= f.text_area :address %>
  </div>
  <%= f.submit "Create", class: "btn btn-large btn-primary" %>
<% end %>